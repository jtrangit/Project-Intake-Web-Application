@model List<CapstoneProject.Models.ClassLibrary.Project>
@{
    ViewData["Title"] = "Edit Profile";
    Layout = "_ClientLayout";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Pending Projects</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/GridView.css" asp-append-version="true" />
</head>
<body>

    <div class="container mt-5">
        <h3 class="text-center mb-4">Pending Projects</h3>

        <div class="row">
            <div class="col-lg-12">
                <div class="table-responsive">
                    <table class="table table-striped table-hover shadow-sm">
                        <thead class="table-dark">
                            <tr>
                                <th>Name</th>
                                <th>Description</th>
                                <th>Submitted Date</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Class Registration</td>
                                <td>An updated way to register for classes</td>
                                <td>2/20/2024</td>
                                <td>
                                    <a href="#" class="btn btn-primary">View Project</a>
                                </td>
                            </tr>
                            <tr>
                                <td>Food Truck App</td>
                                <td>Ordering from food trucks on campus through GrubHub</td>
                                <td>2/25/2024</td>
                                <td>
                                    <a href="#" class="btn btn-primary">View Project</a>
                                </td>
                            </tr>
                            <tr>
                                <td>Bathroom Finder App</td>
                                <td>Finds the nearest bathroom on campus</td>
                                <td>3/3/2024</td>
                                <td>
                                    <a href="#" class="btn btn-primary">View Project</a>
                                </td>
                            </tr>
                            <tr>
                                <td>Temple Merch</td>
                                <td>Open Locations selling temple merchandise</td>
                                <td>3/10/2024</td>
                                <td>
                                    <a href="#" class="btn btn-primary">View Project</a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
     <form asp-controller="Account" asp-action="CreateNewProject" method="post">
    <div class="text-center mt-4">
        <button type="submit" class="btn btn-success btn-lg">Create New Project</button>
    </div>
    <div style="height: 200px;"></div>
</form>

       
       
    

</body>
</html>
